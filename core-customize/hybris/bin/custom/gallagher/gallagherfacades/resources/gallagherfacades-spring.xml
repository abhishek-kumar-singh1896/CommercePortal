<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config />

	<alias name="gallagherVariantOptionDataMediaPopulator" alias="variantOptionDataMediaPopulator" />
	<bean id="gallagherVariantOptionDataMediaPopulator"
		class="com.gallagher.facades.product.converters.populator.variantoptions.GallagherVariantOptionDataMediaPopulator">
		<property name="acceleratorImageFormatMapping" ref="imageFormatMapping" />
		<property name="imageFormats">
			<list>
				<value>zoom</value>
				<value>product</value>
				<value>thumbnail</value>
				<value>cartIcon</value>
				<value>styleSwatch</value>
			</list>
		</property>
	</bean>

	<alias name="gallagherB2BCustomerReversePopulator" alias="b2BCustomerReversePopulator" />
	<bean id="gallagherB2BCustomerReversePopulator"
		class="com.gallagher.facades.company.converters.populators.GallagherB2BCustomerReversePopulator"
		parent="defaultB2BCustomerReversePopulator" />


	<alias name="gallagherSearchResultProductPopulator" alias="commerceSearchResultProductPopulator" />
	<bean id="gallagherSearchResultProductPopulator"
		class="com.gallagher.facades.search.converters.poulator.GallagherSearchResultProductPopulator"
		parent="variantCommerceSearchResultProductPopulator">
	</bean>

	<alias name="gallagherProductCategorySearchPagePopulator" alias="productCategorySearchPagePopulator" />
	<bean id="gallagherProductCategorySearchPagePopulator"
		class="com.gallagher.facades.search.converters.poulator.GallagherProductCategorySearchPagePopulator"
		parent="defaultProductCategorySearchPagePopulator">
	</bean>

	<!-- gallagher Image Format Mappings -->
	<alias name="gallagherImageFormatMapping" alias="imageFormatMapping" />
	<bean id="gallagherImageFormatMapping" parent="acceleratorImageFormatMapping">
		<property name="mapping">
			<map merge="true">
				<entry key="thumbnailb2c" value="167Wx120H" />
				<entry key="productb2c" value="500Wx500H" />
				<entry key="productb2b" value="480Wx480H" />
			</map>
		</property>
	</bean>

	<alias name="gallagherProductGalleryImagesPopulator" alias="productGalleryImagesPopulator" />
	<bean id="gallagherProductGalleryImagesPopulator" parent="defaultProductGalleryImagesPopulator"
		class="com.gallagher.facades.product.converters.populator.GallagherProductGalleryImagesPopulator">
		<property name="imageFormats">
			<list>
				<value>superZoom</value>
				<value>zoom</value>
				<value>product</value>
				<value>thumbnail</value>
				<value>thumbnailb2c</value>
				<value>productb2c</value>
				<value>productb2b</value>
			</list>
		</property>
	</bean>

	<alias name="gallagherProductPrimaryImagePopulator" alias="productPrimaryImagePopulator" />
	<bean id="gallagherProductPrimaryImagePopulator" parent="defaultProductPrimaryImagePopulator"
		class="com.gallagher.facades.product.converters.populator.GallagherProductPrimaryImagePopulator">
		<property name="imageFormats">
			<list>
				<value>zoom</value>
				<value>product</value>
				<value>thumbnail</value>
				<value>cartIcon</value>
				<value>thumbnailb2c</value>
				<value>productb2c</value>
				<value>productb2b</value>
			</list>
		</property>
	</bean>

	<bean id="gallagherProductOthersPopulator" parent="baseProductPopulator"
          class="com.gallagher.facades.product.converters.populator.GallagherProductOthersPopulator">
    </bean>
	
    <bean id="gallagherProductImageDescPopulator" parent="baseProductPopulator"
          class="com.gallagher.facades.product.converters.populator.GallagherProductImageDescriptionPopulator">
    </bean>
    
    <bean id="gallagherProductVideoDescPopulator" parent="baseProductPopulator"
          class="com.gallagher.facades.product.converters.populator.GallagherProductVideoDescriptionPopulator">
    </bean>
    
    <bean id="gallagherProductLogoPopulator" parent="baseProductImagePopulator"
          class="com.gallagher.facades.product.converters.populator.GallagherProductLogoPopulator">
    </bean>
    
    <bean id="gallagherProductDataSheetPopulator" parent="baseProductImagePopulator"
          class="com.gallagher.facades.product.converters.populator.GallagherProductDataSheetPopulator">
    </bean>
    
    <bean id="gallagherProductSimulatorPopulator" parent="baseProductPopulator"
          class="com.gallagher.facades.product.converters.populator.GallagherProductSimulatorPopulator">
    </bean>
    
    <bean parent="configurablePopulatorModification">
		<property name="target" ref="productConfiguredPopulator" />
		<property name="keyType" value="de.hybris.platform.commercefacades.product.ProductOption" />
		<property name="key" value="SIMULATOR" />
		<property name="add" ref="gallagherProductSimulatorPopulator" />
	</bean>
    
    <bean parent="configurablePopulatorModification">
		<property name="target" ref="productConfiguredPopulator" />
		<property name="keyType" value="de.hybris.platform.commercefacades.product.ProductOption" />
		<property name="key" value="OTHERS" />
		<property name="add" ref="gallagherProductOthersPopulator" />
	</bean>
    
    <bean parent="configurablePopulatorModification">
		<property name="target" ref="productConfiguredPopulator" />
		<property name="keyType" value="de.hybris.platform.commercefacades.product.ProductOption" />
		<property name="key" value="IMAGE_DESCRIPTION" />
		<property name="add" ref="gallagherProductImageDescPopulator" />
	</bean>
    
    <bean parent="configurablePopulatorModification">
		<property name="target" ref="productConfiguredPopulator" />
		<property name="keyType" value="de.hybris.platform.commercefacades.product.ProductOption" />
		<property name="key" value="VIDEO_DESCRIPTION" />
		<property name="add" ref="gallagherProductVideoDescPopulator" />
	</bean>

	<bean parent="configurablePopulatorModification">
		<property name="target" ref="productConfiguredPopulator" />
		<property name="keyType" value="de.hybris.platform.commercefacades.product.ProductOption" />
		<property name="key" value="LOGO" />
		<property name="add" ref="gallagherProductLogoPopulator" />
	</bean>

	<bean parent="configurablePopulatorModification">
		<property name="target" ref="productConfiguredPopulator" />
		<property name="keyType" value="de.hybris.platform.commercefacades.product.ProductOption" />
		<property name="key" value="DATA_SHEET" />
		<property name="add" ref="gallagherProductDataSheetPopulator" />
	</bean>

	<bean id="gallagherRegionDetectionFacade" class="com.gallagher.facades.impl.GallagherRegionDetectionFacadeImpl" />

	<alias name="gallagherProductReferencePopulator" alias="productReferencePopulator" />
	<bean id="gallagherProductReferencePopulator" parent="defaultProductReferencePopulator"
		class="com.gallagher.facades.product.converters.populator.GallagherProductReferencePopulator">
		<property name="productConverter" ref="productConverter"></property>
		<property name="classificationPopulator" ref="productClassificationPopulator"></property>
	</bean>

	<alias name="gallagherProductPopulator" alias="productPopulator" />
	<bean id="gallagherProductPopulator" class="com.gallagher.facades.product.converters.populator.GallagherProductPopulator"
		parent="defaultProductPopulator">
		<property name="productPricePopulator" ref="productPricePopulator" />
		<property name="productGalleryImagesPopulator" ref="productGalleryImagesPopulator" />
	</bean>

	<alias name="gallagherCustomerFacade" alias="customerFacade" />
	<bean id="gallagherCustomerFacade" class="com.gallagher.facades.customer.impl.GallagherCustomerFacadeImpl"
		parent="defaultCustomerFacade" />

	<bean id="gallagherCustomerEmailContext" class="com.gallagher.facades.process.email.context.GallagherCustomerEmailContext"
		parent="customerEmailContext" />
   
   <bean id="gallagherB2BUserGroupFacade" class="com.gallagher.facades.usergroups.GallagherB2BUserGroupFacade" parent="defaultB2BUserGroupFacade" />
    
    <bean parent="modifyPopulatorList">
        <property name="list" ref="customerConverter"/>
        <property name="add" ref="gallagherB2BCustomerPopulator"/>
    </bean>
    
    <bean id="gallagherB2BCustomerPopulator" class="com.gallagher.facades.company.converters.populators.GallagherB2BCustomerPopulator"	parent="b2bCustomerPopulator" />
	
		

	<bean id="registeredProductData" class="com.gallagher.facades.product.data.RegisteredProductData" scope="prototype" />

	<alias name="gallagherRegisteredProductsPopulator" alias="registeredProductsPopulator" />
	<bean id="gallagherRegisteredProductsPopulator"
		class="com.gallagher.facades.customer.converters.populator.GallagherRegisteredProductsPopulator" >
		<property name="searchRestrictionService" ref="searchRestrictionService"/>
		<property name="sessionService" ref="sessionService" />
		<property name="baseSiteService" ref="baseSiteService" />
		<property name="catalogVersionService" ref="catalogVersionService" />
	</bean>

	<alias name="gallagherRegisteredProductsConverter" alias="registeredProductsConverter" />
	<bean id="gallagherRegisteredProductsConverter" parent="abstractPopulatingConverter">
		<property name="targetClass" value="com.gallagher.facades.product.data.RegisteredProductData" />
		<property name="populators">
			<list>
				<ref bean="registeredProductsPopulator" />
			</list>
		</property>
	</bean>

	<alias name="gallagherRegisteredProductsFacade" alias="registeredProductsFacade" />
	<bean id="gallagherRegisteredProductsFacade" class="com.gallagher.facades.impl.GallagherRegisteredProductsFacadeImpl">
		<property name="registeredProductsConverter" ref="registeredProductsConverter" />
	</bean>
	
	<alias name="gallagherOrdermanagementProductPopulator" alias="ordermanagementProductPopulator" />
	<bean id="gallagherOrdermanagementProductPopulator"
			class="com.gallagher.facades.product.converters.populator.GallagherOrdermanagementProductBasicPopulator"/>

	<alias name="gallagherProductBasicPopulator" alias="productBasicPopulator"/>
	<bean id="gallagherProductBasicPopulator" parent="defaultProductBasicPopulator"
   		class="com.gallagher.facades.product.converters.populator.GallagherProductBasicPopulator" />
          
	<alias name="gallagherProductUrlPopulator" alias="productUrlPopulator"/>
	<bean id="gallagherProductUrlPopulator" parent="defaultProductUrlPopulator"
			class="com.gallagher.facades.product.converters.populator.GallagherProductUrlPopulator" />

	<alias name="gallagherGroupOrderEntryPopulator" alias="groupOrderEntryPopulator"/>
	<bean id="gallagherGroupOrderEntryPopulator" parent="defaultGroupOrderEntryPopulator"
			class="com.gallagher.facades.order.converters.populator.GallagherGroupOrderEntryPopulator"/>
	
	<alias name="gallagherB2BUnitFacade" alias="b2bUnitFacade" />
	<bean id="gallagherB2BUnitFacade" parent="defaultB2BUnitFacade"
		class="com.gallagher.facades.impl.GallagherB2BUnitFacadeImpl">
	</bean>		
	
	<alias name="gallagherB2BUnitPopulator" alias="b2bUnitPopulator" />
	<bean id="gallagherB2BUnitPopulator" parent="defaultB2BUnitPopulator"
		class="com.gallagher.converters.populators.impl.GallagherB2BUnitPopulatorImpl">
	</bean>

	<alias name="gallagherCreditCardPaymentInfoPopulator" alias="creditCardPaymentInfoPopulator" />
	<bean id="gallagherCreditCardPaymentInfoPopulator"
		class="com.gallagher.facades.order.converters.populator.GallagherCreditCardPaymentInfoPopulator" parent="defaultCreditCardPaymentInfoPopulator" />
		
	<alias name="gallagherStoreSessionFacade" alias="storeSessionFacade"/>
    <bean id="gallagherStoreSessionFacade"
          class="com.gallagher.facades.storesession.impl.GallagherStoreSessionFacadeImpl" parent="defaultStoreSessionFacade"/>
          
    <alias name="gallagherDefaultCheckoutFacade" alias="acceleratorCheckoutFacade"/>
    <bean id="gallagherDefaultCheckoutFacade" class="com.gallagher.facades.checkout.GallagherDefaultCheckoutFacadeImpl" parent="defaultAcceleratorCheckoutFacade" />

	<alias name="gallagherB2BUserFacade" alias="b2bUserFacade" />
	<bean id="gallagherB2BUserFacade" class="com.gallagher.facades.company.impl.GallagherB2BUserFacadeImpl"
		parent="defaultB2BUserFacade">
		<property name="eventService" ref="eventService" />
		<property name="baseSiteService" ref="baseSiteService" />
		<property name="baseStoreService" ref="baseStoreService" />
		<property name="commonI18NService" ref="commonI18NService" />
	</bean>
	
	<alias name="gallagherCmsLinkComponentValidator" alias="cmsLinkComponentValidator"/>
	<bean id="gallagherCmsLinkComponentValidator" class="com.gallagher.facades.cmsitems.validator.GallagherCMSLinkComponentValidator"
			parent="defaultCmsLinkComponentValidator"/>
			
	<alias name="gallagherCategoryPopulator" alias="categoryPopulator"/>
    <bean id="gallagherCategoryPopulator"
          class="com.gallagher.facades.commercefacades.product.converters.populator.GallagherCategoryPopulator"
          parent="defaultCategoryPopulator"/>
 	  
 		<!-- Urlencoder -->
	<alias name="gallagherUrlEncoderFacade" alias="urlEncoderFacade"/>
	<bean id="gallagherUrlEncoderFacade" class="com.gallagher.facades.urlencoder.impl.GallagherUrlEncoderFacadeImpl" parent="defaultUrlEncoderFacade"/>
</beans>
