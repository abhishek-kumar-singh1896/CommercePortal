<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<enum class="de.hybris.platform.commercefacades.product.ProductOption">
		<value>VIDEO_DESCRIPTION</value>
		<value>LOGO</value>
		<value>DATA_SHEET</value>
		<value>IMAGE_DESCRIPTION</value>
		<value>OTHERS</value>
		<value>SIMULATOR</value>
	</enum>

	<enum class="de.hybris.platform.commercefacades.product.data.ImageDataType">
		<value>LOGO</value>
		<value>DATASHEET</value>
		<value>OTHERS</value>
	</enum>
	
  	<!-- Add custom categories to ticket -->	
	<enum class="de.hybris.platform.customerticketingfacades.data.TicketCategory">
        <value>SECURITY_TECHNICAL_SUPPORT</value>		          
        <value>SECURITY_SALES</value>
	</enum>	

	<bean class="de.hybris.platform.commercefacades.user.data.CustomerData">
		<property name="duplicate" type="java.lang.Boolean" />
		<property name="keycloakGUID" type="java.lang.String" />
		<property name="objectID" type="java.lang.String" />
		<property name="isUserExist" type="boolean" />
		<property name="newsLetters" type="java.lang.Boolean" />
       <property name="productRelease" type="java.lang.Boolean"/>
		<property name="events" type="java.lang.Boolean" />
		<property name="productUpdate" type="java.lang.Boolean" />
		<property name="productPromo" type="java.lang.Boolean" />
		<property name="loginDisabled" type="java.lang.Boolean"/>
		<property name="units" type="java.util.List&lt;de.hybris.platform.b2bcommercefacades.company.data.B2BUnitData>" />
	</bean>

	<bean class="de.hybris.platform.commercefacades.product.data.ImageData">
		<property name="description" type="java.lang.String" />
		<property name="mime" type="java.lang.String" />
		<property name="size" type="java.lang.String" />
	</bean>

	<bean class="de.hybris.platform.commercefacades.product.data.PDPSimulatorData">
		<property name="simulatorDescription" type="java.lang.String" />
		<property name="simulatorURL" type="java.lang.String" />
	</bean>

	<bean class="de.hybris.platform.commercefacades.product.data.ProductVideoData">
		<property name="description" type="java.lang.String" />
		<property name="url" type="java.lang.String" />
		<property name="id" type="java.lang.String" />
	</bean>

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="promoSticker" type="String" />
		<property name="videoDescription" type="String" />
		<property name="simulator" type="de.hybris.platform.commercefacades.product.data.PDPSimulatorData" />
		<property name="imageDescription" type="String" />
		<property name="videos" type="java.util.List&lt;de.hybris.platform.commercefacades.product.data.ProductVideoData&gt;" />
		<property name="logo" type="java.util.Collection&lt;de.hybris.platform.commercefacades.product.data.ImageData>" />
		<property name="dataSheet" type="java.util.Collection&lt;de.hybris.platform.commercefacades.product.data.ImageData>" />
		<property name="others" type="java.util.Collection&lt;de.hybris.platform.commercefacades.product.data.ImageData>" />
		<property name="animalCompatibility" type="java.util.List&lt;String&gt;" />
		<property name="sparePartsReferenceHeading" type="String" />
		<property name="sparePartsReferenceSubHeading" type="String" />
		<property name="othersReferenceHeading" type="String" />
		<property name="othersReferenceSubHeading" type="String" />
		<property name="plpProductDescription" type="String" />
		<property name="categoryPaths" type="java.util.List&lt;String&gt;" />
		<property name="modifiedTime" type="java.util.Date" />
		<property name="recommendedProducts" type="java.util.List&lt;de.hybris.platform.commercefacades.product.data.ProductData>" />
		<property name="recommendedCategories" type="java.util.List&lt;de.hybris.platform.commercefacades.product.data.CategoryData>" />
		<property name="productSpecificDetailsHeading" type="String" />
<!-- 		<property name="marketingDescription" type="String"/> -->
	</bean>

	<bean class="de.hybris.platform.commerceservices.search.facetdata.ProductCategorySearchPageData&lt;STATE, RESULT, CATEGORY>">
		<property name="description" type="String" />
	</bean>

	<bean class="com.gallagher.facades.product.data.RegisteredProductData">
		<property name="name" type="java.lang.String" />
		<property name="code" type="java.lang.String" />
		<property name="purchaseDate" type="java.util.Date" />
		<property name="registrationDate" type="java.util.Date" />
		<property name="image" type="de.hybris.platform.commercefacades.product.data.ImageData" />
		<property name="attachment" type="java.lang.String" />
		<property name="attachmentUrl" type="java.lang.String" />
	</bean>

    <bean class="de.hybris.platform.b2bcommercefacades.company.data.B2BUnitData" extends="de.hybris.platform.commercefacades.user.data.CompanyData">
        <property name="technicians" type="java.util.List&lt;de.hybris.platform.commercefacades.user.data.CustomerData>"/>
        <property name="transactional" type="boolean"/>
    </bean>
    
    <bean class="de.hybris.platform.commercefacades.order.data.OrderData">
		<property name="formattedOrderDate" type="String"/>
	 </bean>
	 
	 <bean class="de.hybris.platform.commercefacades.order.data.OrderHistoryData">
		<property name="formattedOrderDate" type="String"/>
	</bean>

	<bean class="de.hybris.platform.commercefacades.product.data.ReviewData">
		<property name="formattedReviewDate" type="String" />
	</bean>


	<bean class="de.hybris.platform.commercefacades.order.data.CartData"
		extends="de.hybris.platform.commercefacades.order.data.AbstractOrderData">

		<property name="b2bUnit" type="de.hybris.platform.b2bcommercefacades.company.data.B2BUnitData" />

	</bean>

	<bean class="de.hybris.platform.b2bcommercefacades.company.data.B2BUnitData"
		extends="de.hybris.platform.commercefacades.user.data.CompanyData">
		<property name="code" type="String" />
	</bean>

	<bean class="de.hybris.platform.commercefacades.order.data.OrderData"
		extends="de.hybris.platform.commercefacades.order.data.AbstractOrderData">
		<property name="b2bUnit" type="de.hybris.platform.b2bcommercefacades.company.data.B2BUnitData" />
		<property name="requiredDeliveryDate" type="String" />
	</bean>
	
	<bean class="de.hybris.platform.commercefacades.order.data.OrderEntryData">
		<property name="deliveredQuantity" type="Long" />
		<property name="status" type="com.gallagher.core.enums.OrderEntryItemStatus" />
	</bean>

	<bean class="de.hybris.platform.commercefacades.order.data.CartData">
		<property name="requiredDeliveryDate" type="String" />
	</bean>
	
	<bean class="de.hybris.platform.solrfacetsearch.config.IndexedProperty">
		<property name="salesArea" type="boolean"/>
		<property name="userPriceGroup" type="boolean"/>
	</bean>
	<bean class="de.hybris.platform.commercefacades.order.data.DeliveryInstructionData">
		<property name="deliveryinstruction" type="String" />
		<property name="entryNumber" type="Integer" />
		<property name="productSpecificDetailsHeading" type="String" />
	</bean>
	
	<bean class="de.hybris.platform.commercefacades.order.data.OrderEntryData">
		<property name="deliveryinstruction" type="String" />
		<property name="productSpecificDetailsHeading" type="String" />
	</bean>
	
	<bean class="de.hybris.platform.commercefacades.order.data.AbstractOrderData">
		<property name="deliveryInstruction" type="String" />
	</bean>
	
	<bean class="de.hybris.platform.commercefacades.product.data.CategoryData">
		<property name="commentsQuestion" type="String" />
	</bean>
</beans>
