<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<!-- SapCpiOrderPriceComponent -->
	<bean class="de.hybris.platform.sap.sapcpiadapter.data.SapCpiOrderPriceComponent">
		<property name="totalAmt" type="String" />
	</bean>


	<bean class="de.hybris.platform.sap.sapcpiadapter.data.SapCpiOrderAddress">
		<property name="taxJurCode" type="String" />
		<property name="gallagherStreetNumber" type="String" />
	</bean>

	<!-- SapCpiCreditCardPayment -->
	<bean class="de.hybris.platform.sap.sapcpiadapter.data.SapCpiCreditCardPayment">
		<property name="amount" type="String" />
		<property name="authorizationTime" type="String" />
		<property name="authorizationNumber" type="String" />
		<property name="resultText" type="String" />
		<property name="merchantID" type="String" />
		<property name="avsCode" type="String" />
	</bean>
	
	<bean class="de.hybris.platform.sap.sapcpiadapter.data.SapCpiOrder">
		<property name="requiredDeliveryDate" type="String" />
		<property name="comment" type="String" />
	</bean>

	<bean class="de.hybris.platform.sap.sapcpiadapter.data.SapCpiOrderItem">
		<property name="additionalProductDetail" type="String" />
	</bean>

</beans>